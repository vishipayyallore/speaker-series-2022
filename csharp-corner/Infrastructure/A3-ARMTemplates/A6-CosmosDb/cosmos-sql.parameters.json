{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accountName": {
            "value": "[format('cosmos-sql-{0}', uniqueString(resourceGroup().id))]"
        },
        "location": {
            "value": "[resourceGroup().location]"
        },
        "primaryRegion": {
            "value": "eastus"
        },
        "secondaryRegion": {
            "value": "westus"
        },
        "defaultConsistencyLevel": {
            "value": "Session"
        },
        "maxStalenessPrefix": {
            "value": 100000
        },
        "maxIntervalInSeconds": {
            "value": 300
        },
        "systemManagedFailover": {
            "value": true
        },
        "databaseName": {
            "value": "eShopDb"
        },
        "containerName": {
            "value": "productsContainer"
        },
        "throughput": {
            "value": 400
        }
    }
}